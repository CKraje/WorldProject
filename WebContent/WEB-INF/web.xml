<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>WorldProject</display-name>
	<servlet>
		<servlet-name>countries</servlet-name>
		<servlet-class>it.objectmethod.world.servlet.CountryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>countries</servlet-name>
		<url-pattern>/countries</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>continenti</servlet-name>
		<servlet-class>it.objectmethod.world.servlet.ContinentsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>continenti</servlet-name>
		<url-pattern>/continents</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>cities</servlet-name>
		<servlet-class>it.objectmethod.world.servlet.CityServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cities</servlet-name>
		<url-pattern>/cities</url-pattern>
	</servlet-mapping>
</web-app>